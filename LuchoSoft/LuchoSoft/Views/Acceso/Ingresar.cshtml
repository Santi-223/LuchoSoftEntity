@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="~/css/gestionDeAcceso.css">
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
</head>

<body>
    <div class="contenido">
        <center>

            <div id="titulo">
                <h1>Bienvenido</h1>
            </div>
            <div id="perfil">
                <div id="imgPerfil"><img src="~/lib/imagenes/Profile.png" height="70vh" lenght="70vh"></div>

            </div>
        </center>

        <div class="divActualizarContraseña">
            <form>
                <div class="input-group">
                    <label for="contrasenaAntigua">Ingrese correo</label>
                    <input type="email" id="correoacceso" required>
                </div>

                <div class="input-group">
                    <label for="confirmarContrasena">Ingrese contraseña</label>
                    <input type="password" id="contrasenaacceso" required>
                </div>
                <div class="cajaBotonesRegistro">
                    <button class="rojo" type="button" onclick="validacionacceso()">Acceder</button>
                </div>
            </form>
        </div>



    </div>
</body>

<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/js/validacionacceso.js"></script>
</html>